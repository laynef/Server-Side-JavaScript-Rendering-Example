doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0')
        meta(name='og:description', content='Description')
        meta(name='og:author', content='Layne Faler')
        meta(property='og:type', content='website')
        meta(http-equiv='cache-control', content='public')
        meta(http-equiv='language', content='en-us')
        meta(name='robots', content='all')
        meta(property='og:site_name', content='Demo')
        meta(property='og:url', content='https://www.example.com')
        meta(name='viewport' content='user-scalable=no, width=device-width')
        meta(property='og:image', content=`/assets/img/favicon.ico?id=${hashId}`)
        meta(name='apple-mobile-web-app-capable', content='yes')
        meta(name='apple-mobile-web-app-status-bar-style', content='black')
        meta(property='keywords', content='javascript')
        title='Demo'
        link(rel='apple-touch-icon' href=`/assets/img/favicon.ico?id=${hashId}`)
        link(rel='apple-touch-startup-image' href=`/assets/img/favicon.ico?id=${hashId}`)
        link(type='image/x-icon', rel='apple-touch-icon-precomposed', href=`/assets/img/favicon.ico?id=${hashId}`)
        link(type='image/x-icon', rel='shortcut icon', href=`/assets/img/favicon.ico?id=${hashId}`)
        link(type='image/x-icon', rel='icon', href=`/assets/img/favicon.ico?id=${hashId}`)
        link(href=`/assets/dist/main.css?id=${hashId}`, rel='stylesheet' type='text/css')
    body
        h1='400'
        h2='Not Found'
