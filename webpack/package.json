    "scripts": {
        "start": "concurrently \"npm run api-start\" \"npm run client-start\"",
        "client-start": "node server.js",
        "api-start": "concurrently \"redis-server --port 6381\" \"node app.js\"",
        "lint": "eslint -c .eslintrc web api models migrations app.js --fix",
        "test": "karma start",
        "postinstall": "webpack --config ./webpack/client.prod.config.js -p && webpack --config ./webpack/server.prod.config.js -p",
        "build": "concurrently \"webpack --config ./webpack/client.prod.config.js -p\" \"webpack --config ./webpack/server.prod.config.js -p\"",
        "dev": "concurrently \"npm run api-dev\" \"npm run client-dev\"",
        "client-dev": "concurrently \"webpack --config ./webpack/client.dev.config.js -w\"  \"nodemon server.js\" \"webpack --config ./webpack/server.dev.config.js -w\"",
        "api-dev": "concurrently \"nodemon app.js\" \"redis-server --port 6381\"",
        "clean": "concurrently \"rm -rf uploads/\" \"rm -rf assets/dist\"",
        "database": "redis-cli flushall && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all && concurrently \"npm run api-dev\""
